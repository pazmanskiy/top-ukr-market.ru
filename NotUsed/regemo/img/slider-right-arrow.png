			var form = self.closest('form');
			var phoneFields = $(phoneFieldSelector, form);
			
			if (phoneFields.length) {
				self.click(function(event) {
					var phoneFieldIsValidated = true;
					
					phoneFields.each(function() {
						var phoneFieldId = $(this).attr(phoneFieldIdAttributeName);						
						if (document.phoneFieldIsValidated[phoneFieldId] != true) {
							phoneFieldIsValidated = false;	
						}
					});
					
					if (!phoneFieldIsValidated) {
						alert(params.phoneNumberShouldBe6MoreCharacters);
						
						event.preventDefault();
						event.stopPropagation();
					
						return false;
					}
				});
			}
		})
    })
</script></body>

</html>

